{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./public"
    },
    "include": ["./**/*.ts"],
    "exclude": ["node_modules"],
    "noUnusedLocals": false,
    "noUnusedParameters": false
}